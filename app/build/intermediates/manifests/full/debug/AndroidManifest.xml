<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.easyar.samples.helloarvideo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="视频教学"
        android:theme="@style/AppTheme" >
        <activity
            android:name="cn.easyar.samples.helloarvideo.LaunchActivity"
            android:configChanges="orientation|keyboardHidden|screenSize|smallestScreenSize"
            android:label="@string/app_name"
            android:screenOrientation="unspecified" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="cn.easyar.samples.helloarvideo.WebActivity" />
        <activity android:name="cn.easyar.samples.helloarvideo.ArActivity" />
        <activity android:name="cn.easyar.samples.helloarvideo.MainActivity" />
    </application>

</manifest>